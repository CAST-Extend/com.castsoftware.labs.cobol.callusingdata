<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>com.castsoftware.labs.cobol.callusingdata</id>
    <version>1.0.1</version>
    <title>COBOL Data sharing via CALL USING statements</title>
    <authors>CAST Labs</authors>
    <owners>CAST Labs</owners>
    <licenseUrl>https://help.castsoftware.com/hc/en-us/articles/212612648-AIP-Extension-License</licenseUrl>
    <projectUrl>https://github.com/CAST-Extend/com.castsoftware.labs.cobol.callusingdata</projectUrl>
    <iconUrl>https://github.com/CAST-Extend/com.castsoftware.labs.cobol.callusingdata/blob/f3b8824de9865cb58937da4f6a64e283007022f9/com.castsoftware.labs.cobol.callusingdata.png</iconUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>This extension helps understand the Cobol Data interface used by two Cobol Programs that are calling each other with a CALL USING statement: it will create REFER links between the matching Cobol Data items on callee and caller side.</description>
    <releaseNotes>https://github.com/CAST-Extend/com.castsoftware.labs.cobol.callusingdata/wiki</releaseNotes>
    <dependencies>
      <dependency id="CAIP" version="8.3.26" />
      <dependency id="com.castsoftware.mainframe" version="1.0.18-funcrel" />
    </dependencies>
  </metadata>
</package>
